{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "RegionCheck",
      "Effect": "Deny",
      "NotAction": [
        "iam:*",
        "route53:*"
      ],
      "Resource": "*",
      "Condition": {
        "ForAllValues:StringNotEquals": {
          "aws:RequestedRegion": [
            "eu-west-1"
          ]
        }
      }
    }
  ]
}
